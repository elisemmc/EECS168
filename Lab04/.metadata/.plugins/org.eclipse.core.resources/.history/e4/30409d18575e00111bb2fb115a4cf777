public class Q3{
	public static void main(String[] args){
		int cnt =1;
		double Psum=0;
		int n = 1;
		while (cnt <= 10){
			int i = cnt * 5000;
			while (n <= i){
				double num2 = 1/(n-1)-1/(n+1);
				Psum += num2;
				n++;
				if (n == i){
					System.out.printf("%2.5d", (4 * Psum));
				}
			}
			cnt ++;
		}
			
	}
}