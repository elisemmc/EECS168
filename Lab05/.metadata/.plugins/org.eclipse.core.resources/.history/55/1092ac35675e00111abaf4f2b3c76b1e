public class Pyramid {
	public static void main(String[] args){;
		int A = 5;
		for (int cnt = 1; cnt <= 5; cnt++){
			System.out.println("");
			for (int j =1; j <= 5; j++){
				System.out.print("*");
				A--;
			}
		}	
	}

}
